{
  "name": "mantine-vite-template",
  "private": true,
  "license": "AGPLv3-only",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "getEntries": "tsc --module nodenext --outDir build script/getEntries.mts && node build/getEntries.mjs",
    "getResults": "node --loader ts-node/esm script/getResults.mts",
    "getAvatars": "tsc --module nodenext --outDir build script/getAvatars.mts && node build/getAvatars.mjs",
    "getBlurbs": "tsc --module nodenext --outDir build script/getBlurbs.mts && node build/getBlurbs.mjs",
    "getStats": "node --loader ts-node/esm script/getStats.mts",
    "getLeaderboard": "node --loader ts-node/esm script/getLeaderboard.mts",
    "closeEntries": "node --loader ts-node/esm script/closeEntries.mts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run jest && npm run build"
  },
  "prettier": {
    "printWidth": 160,
    "singleQuote": true
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^6.0.10",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^6.0.10",
    "@mantine/modals": "^6.0.10",
    "badwords-filter": "^1.3.0",
    "bardapi": "^1.0.4",
    "chatgpt": "^5.2.4",
    "dotenv": "^16.0.3",
    "gm": "^1.25.0",
    "googlethis": "^1.7.1",
    "pdfjs-dist": "^3.5.141",
    "pngjs": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "tabler-icons-react": "^1.56.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/better-sqlite3": "^7.6.3",
    "@types/gm": "^1.25.0",
    "@types/jest": "^29.4.0",
    "@types/pngjs": "^6.0.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "better-sqlite3": "^8.2.0",
    "csv-parse": "^5.3.6",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0",
    "name-fixer": "^1.0.0",
    "pdf2json": "^3.0.3",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  }
}
